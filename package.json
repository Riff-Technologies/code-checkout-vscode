{
  "name": "@riff-tech/code-checkout",
  "version": "1.2.9",
  "publisher": "riff-tech",
  "description": "Code Checkout support for paid VSCode Extensions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "code-checkout-init": "./dist/scripts/install.js",
    "code-checkout-build": "./dist/scripts/build.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "build:vsix": "npm run build && node dist/scripts/build.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && chmod +x ./dist/scripts/install.js",
    "install-extension": "node ./dist/scripts/install.js",
    "pack:local": "rm -rf dist && npm pack",
    "dev-setup": "./dev-setup.sh ../testmystuff",
    "lint": "eslint 'src/**/*.{ts,tsx}' --quiet",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --quiet --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "publish": "npm run build && npm publish --access public"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Riff-Technologies/code-checkout.git"
  },
  "author": "Riff Tech, LLC",
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/vscode": "^1.39.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "glob": "^7.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "glob": "^7.2.0",
    "javascript-obfuscator": "^4.1.1"
  },
  "volta": {
    "node": "20.16.0",
    "yarn": "1.22.15"
  }
}
